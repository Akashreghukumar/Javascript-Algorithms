let nums=[1,2,3,6,1]
let flag
for(let i=0;i<=nums.length-1;i++)
{
  for(let j=i+1;j<=nums.length-1;j++)
  {
    // console.log(nums[i],nums[j])
    if(nums[i]==nums[j])
    {
      // console.log("true")
      flag=true
      break
    }
    else
    {
      flag=false
    }
    
    
  }
  if(flag==true)
  {
    break
  }
}

console.log(flag)
