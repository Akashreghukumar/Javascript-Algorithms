matrix=[[1,1,1],[1,0,1],[1,1,1]]
let newmat=[]
// console.log(matrix)

for(let i=0;i<=matrix.length-1;i++)
{
  for(let j=0;j<=matrix[i].length-1;j++)
  {
    if(matrix[i][j]==0)
    {
      for(let k=0;k<=matrix[i].length-1;k++)
      {
        matrix[i][k]=0
      }
      
    }
  
    
  }
  console.log(matrix[i])
}

 
