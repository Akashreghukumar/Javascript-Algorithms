nums = [2,0,2,1,1,0]
console.log(nums)
// console.log(nums.sort())
let temp
for(let i=0;i<=nums.length-1;i++)
{
  for(let j=0;j<=nums.length-1;j++)
  {
    // console.log("before up",nums[i])
  if(nums[i]<nums[j])
  {
    temp=nums[i]
    nums[i]=nums[j]
    nums[j]=temp
  }
  
    // console.log("after up",nums[i])
  }
  
}

console.log(nums)

